{
	"name": "atomita/wp-composer-template",
	"authors": [
		{
			"name": "atomita"
		}
	],
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "wordpress", "version": "3.8.1",
				"type": "webroot",
				"dist": {
					"type": "zip",
					"url": "https://github.com/WordPress/WordPress/archive/3.8.1.zip"
				}
			}
		},
		{
			"type": "composer",
			"url": "http://wpackagist.org"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"wordpress": "3.8.1",
		"fancyguy/webroot-installer": "1.0.0",
		"atomita/wp-environment-config": "dev-master",
		"wpackagist/advanced-custom-fields": "*"
	},
	"extra": {
		"webroot-dir": "html/wp",
		"webroot-package": "wordpress",
		"installer-paths": {
			"html/wp/wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
		}
	}
}
